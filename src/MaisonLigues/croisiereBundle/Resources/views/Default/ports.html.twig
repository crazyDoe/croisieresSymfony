{% extends 'MaisonLiguesCroisiereBundle:Default:base.html.twig' %}
{% block corps %}
<div id="page-wrapper">
	<div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Les Ports</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
	<div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                	<th> ID </th>
                    <th> Nom Port </th>
                </tr>
            </thead>
            <tbody>
                {% for port in lesPorts %}
				<tr>
					<td> {{port.getId()}}</td>
					<td> {{port.getNom()}}</td>
				</tr>
				{% endfor %}
    		 </tbody>
        </table>
    </div>
</div>
{% endblock %}