{% extends 'MaisonLiguesCroisiereBundle:Default:base.html.twig' %}
{% block corps %}
<div id="page-wrapper">
	<div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Choisir votre Traversee</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
<form class="col-sm-4" method="post" action="" role="form">
	<div class="form-group">
	{{form_start(form)}}
		<div class="form-group">
			<label> Date : </label>{{ form_widget(form.Date) }}
		</div>
		<div class="form-group">
			<label> Liaison: </label>{{ form_widget(form.lesLiaison) }}
		</div>
		<input class="form-control" type='submit' value='Chercher'/>
	{{ form_end(form) }}
	</div>
</form>
{% endblock %}